{
  "name": "react-flask-authentication-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-server": ".venv/bin/flask run --no-debugger",
    "server": ".venv/bin/gunicorn -b 127.0.0.1:5000 api:app",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "heroku-postbuild": "npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.5",
    "3dmol": "^2.0.3",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "openchemlib": "^8.3.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.2",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "daisyui": "^3.5.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  },
  "heroku-run-build-script": true
}
